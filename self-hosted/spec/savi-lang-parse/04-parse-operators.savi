:module _Operators
  :fun example_ordered // in order of precedence, from "weakest" to "strongest"
    y = x <<= x x
      && x || x
      === x == x !== x != x =~ x
      >= x <= x < x > x
      <|> x <~> x <<~ x ~>> x << x >> x <~ x ~> x
      + x - x
      * x / x.y

  :fun example_mixed
    a != b && c > d / x + e / y || i.j > k << l

  :fun example_prefix
    ~x
